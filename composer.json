{
  "name": "braunstetter/translated-forms",
  "description": "Translate your forms. Simple and easy.",
  "type": "symfony-bundle",
  "keywords": ["form", "translate"],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "symfony/framework-bundle": "^5.3|^6.0",
    "knplabs/doctrine-behaviors": "^2",
    "symfony/property-access":  "^6",
    "symfony/form": "^6",
    "symfony/security-bundle": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Braunstetter\\TranslatedForms\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Braunstetter\\TranslatedForms\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "symfony/test-pack": "^1.0",
    "matthiasnoback/symfony-dependency-injection-test": "^4.2",
    "nyholm/symfony-bundle-test": "1.8.1",
    "symfony/browser-kit": "^6.0",
    "symfony/twig-bundle": "^5.3|^6.0",
    "symfony/serializer-pack": "^1.1"
  }
}
